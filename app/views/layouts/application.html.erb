<!DOCTYPE html>
<html>
<head>
  <title>Docs</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <script src="http://js.pusher.com/1.11/pusher.min.js"></script>
</head>
<body>
  <% if current_user %>
    <div id=user-login style='display:none'><%= current_user.id%></div>
  <% end %>
  <div id=container>
    <div id=header style='background-attachment:scroll'>
      <div class=centered style='padding-top:22px'>
        <span id=logo>Dockt</span>
        <div id=headerRight>
          <input type='text' name='search' id='search' size='40' value='Search'>
          <%= render 'users/user_bar' %>
        </div>
      </div>
    </div>
    <div id=breadcrumbs>
      <div class=centered style='height:100%;'> 
        <div id='toolbar'>
          <%= render 'layouts/toolbar' %>
        </div>
      </div>
    </div>
    <div id=content class=centered>
      <div id=roundedbox>
        <div id=roundedboxcontent>
          <h1>Collaborate and Annotate.</h1>
          <p>Get all y'alls stuff on here. </p>
        </div>
      </div>
      <div id=contentbox>
        <div id=roundedboxcontent>
          <h1>What</h1>
        </div>
      </div>
      <div id=contentbox>
        <div id=roundedboxcontent>
          <h1>What</h1>
        </div>
      </div>
      <div id=lastcontentbox>
        <h1>What</h1>
      </div>
    </div>
  </div>
  <div id=overlay style='opacity:0; display:none'></div>
  <div id=formContainer style='display:none'></div>
</body>
</html>
